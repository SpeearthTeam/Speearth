<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="720.0" minWidth="1280.0" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<left>
				<AnchorPane fx:id="form_container" styleClass="AnchorPane" BorderPane.alignment="CENTER" />
			</left>
         <center>
            <VBox alignment="TOP_CENTER" BorderPane.alignment="CENTER">
               <children>
      				<ListView fx:id="output_lista_servizi" prefHeight="578.0" prefWidth="1280.0" />
                  <Button fx:id="bottone_conferma" alignment="CENTER" mnemonicParsing="false" onAction="#conferma" prefHeight="58.0" prefWidth="159.0" text="Conferma">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </center>
		</BorderPane>
	</center>
	<top>
		<ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<items>
            <Button fx:id="bottone_torna_alla_home" mnemonicParsing="false" onAction="#vaiAllaHome" text="Torna alla Home">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
				<Button fx:id="bottone_scegli_servizio" mnemonicParsing="false" onAction="#vaiAScegliServizio" text="Scegli Servizio">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
				<Button fx:id="bottone_ricerca" disable="true" mnemonicParsing="false" text="Ricerca">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
				<Button fx:id="bottone_riepilogo" mnemonicParsing="false" onAction="#vaiARiepilogoButtonClick" text="Riepilogo">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
			</items>
		</ToolBar>
	</top>
</BorderPane>
